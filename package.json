{
  "name": "zzrouter",
  "version": "0.1.0",
  "description": "A simple router component to make it easy build Single Page Applications.",
  "type": "module",
  "exports": "./src/zzRouter.js",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "router",
    "SPA"
  ],
  "bugs": "david.javapagetemplates@gmail.com",
  "license": "LGPL-3.0",
  "author": "David Cana Lopez",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/davidcana/zzRouter"
  },
  "devDependencies": {
    "esbuild": "0.25.5",
    "genversion": "^3.2.0",
    "google-closure-compiler": "^20240317.0.0",
    "grunt": "^1.6.1",
    "grunt-contrib-qunit": "^10.1.1",
    "http-server": "^14.1.1",
    "qunit": "~2.11.2",
    "qunit-composite": "^2.0.0",
    "zpt": "^0.40.6",
    "zzdom": "^0.2.0"
  },
  "scripts": {
    "test": "grunt qunit",
    "start": "http-server -p 9000 -c-1",
    "version": "genversion --esm --semi src/version.js",
    "build": "esbuild ./src/zzRouter.js --bundle --outfile=./dist/zzRouter.js --format=esm",
    "build-minify": "esbuild ./src/zzRouter.js --bundle --outfile=./dist/zzRouter.min.js --format=esm --minify",
    "build-gz": "gzip -fk ./dist/zzRouter.min.js",
    "build-all": "npm run build && npm run build-minify && npm run build-gz"
  }
}
